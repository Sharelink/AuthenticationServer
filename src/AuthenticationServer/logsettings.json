{
  "Log": {
    "fileLoggers": [
      {
        "name": "Main",
        "layoutFormat": "${date:format=yyyy-MM-dd HH\\:mm\\:ss} ${logger}:${level:uppercase=true}:${message};${exception}",
        "namePattern": "Main",
        "minLevel": "Debug",
        "fileName": "/home/deployment/logs/Auth/8086/main.log"
      },
      {
        "name": "Route",
        "layoutFormat": "${date:format=yyyy-MM-dd HH\\:mm\\:ss} ${message}",
        "namePattern": "Route",
        "logLevel": "Info",
        "fileName": "/home/deployment/logs/Auth/8086/route.log"
      },
      {
        "name": "Error",
        "layoutFormat": "${date:format=yyyy-MM-dd HH\\:mm\\:ss} ${logger}:${message}->${exception}",
        "namePattern": "*",
        "logLevel": "Error",
        "fileName": "/home/deployment/logs/Auth/8086/error.log"
      },
      {
        "name": "Info",
        "layoutFormat": "${date:format=yyyy-MM-dd HH\\:mm\\:ss} ${message}",
        "namePattern": "Info",
        "logLevel": "Info",
        "fileName": "/home/deployment/logs/Auth/8086/info.log"
      },
      {
        "name": "Warning",
        "layoutFormat": "${date:format=yyyy-MM-dd HH\\:mm\\:ss} ${logger}:${message}",
        "namePattern": "*",
        "logLevel": "Warn",
        "fileName": "/home/deployment/logs/Auth/8086/warning.log"
      }
    ]
  }
}
